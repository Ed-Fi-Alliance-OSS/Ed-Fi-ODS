<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/6.0.0" targetNamespace="http://ed-fi.org/6.0.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="Ed-Fi-Core.xsd" />
  <xs:annotation>
    <xs:documentation>===== Candidate Interchange Model =====</xs:documentation>
  </xs:annotation>
  <xs:element name="InterchangeCandidate">
    <xs:annotation>
      <xs:documentation>This interchange defines teacher candidates and contains all of their identifying information.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Credential" type="Credential" />
        <xs:element name="Student" type="Student" />
        <xs:element name="Candidate" type="Candidate" />
        <xs:element name="CandidateIdentity" type="CandidateIdentity" />
        <xs:element name="StudentAcademicRecord" type="StudentAcademicRecord" />
        <xs:element name="CourseTranscript" type="CourseTranscript" />
        <xs:element name="FieldworkExperience" type="FieldworkExperience" />
        <xs:element name="Certification" type="Certification" />
        <xs:element name="CertificationExam" type="CertificationExam" />
        <xs:element name="CertificationExamResult" type="CertificationExamResult" />
        <xs:element name="CredentialEvent" type="CredentialEvent" />
        <xs:element name="Person" type="Person" />
        <xs:element name="FinancialAid" type="FinancialAid" />
        <xs:element name="EducatorPreparationProgram" type="EducatorPreparationProgram" />
        <xs:element name="CandidateRelationshipToStaffAssociation" type="CandidateRelationshipToStaffAssociation" />
        <xs:element name="CandidateEducatorPreparationProgramAssociation" type="CandidateEducatorPreparationProgramAssociation" />
        <xs:element name="StudentSchoolAssociation" type="StudentSchoolAssociation" />
        <xs:element name="StudentSectionAssociation" type="StudentSectionAssociation" />
        <xs:element name="StudentEducationOrganizationAssociation" type="StudentEducationOrganizationAssociation" />
        <xs:element name="StudentEducationOrganizationResponsibilityAssociation" type="StudentEducationOrganizationResponsibilityAssociation" />
        <xs:element name="FieldworkExperienceSectionAssociation" type="FieldworkExperienceSectionAssociation" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>
